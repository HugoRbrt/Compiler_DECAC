// Description:
//     IfElse structure nesting a while structure.
// 
// Results:
//     5
// 
// History: 
//    created on 13/01/2022 

{
    int x = 0;
    if ( true ) {
        while ( x < 5 ) {
	    x = x + 1;
	}
    } else if ( true ) {
        while ( x < 10 ) {
	    x = x + 1;
        }
    } else {
	while ( x < 15 ) {
            x = x + 1;
	}
    }
    println(x);
}
