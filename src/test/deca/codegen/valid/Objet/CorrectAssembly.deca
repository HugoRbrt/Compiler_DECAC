class A {
    int x;

    int beginning(int a)
    asm ( "LOAD R2, R1 \n WINT \n RTS" );

    void end()
    asm ( "WSTR \" vla les flics\"" );
}


{
    A a = new A();
    print(a.beginning(22));
    a.end();
}
