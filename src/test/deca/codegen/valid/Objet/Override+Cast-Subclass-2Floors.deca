// Description:
//      Program with an override
//      but not using it and downcasting
//
// Result:
//      2
//
// History:
//      created on 21/1/2022


class A { void fun() { println(0); } }
class B extends A { void fun() { println(1);}  }
class C extends B { void fun() { println(2);}  }
{
    C c = new C();
    ((A) c).fun();
}
