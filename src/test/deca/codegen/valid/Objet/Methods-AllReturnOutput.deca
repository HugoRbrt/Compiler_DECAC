class A {
    int x;

    A signatureA() {
        return this;
    }

    int signatureInt(int y) {
        return x + y;
    }

    boolean signatureBoolean(int y) {
        return (x == y);
    }

    float signatureFloat(int y) {
        return ( (float) (x+y) );
    }

    void signatureVoidWith() {
	print(" OK2");
	return;
    }

    void signatureVoidWithout() {
        println(" OK3");
    }
}

{
    A instance = new A();
