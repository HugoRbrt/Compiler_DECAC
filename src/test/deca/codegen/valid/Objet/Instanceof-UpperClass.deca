// Description:
//      Program verifying if instanceof is well designed for a subclass
//
// Result:
//      OK
//
// History:
//      created on 17/1/2022


class A { }
class B extends A { int x; }
class C extends B { }

{
    C a;
    a = new C();
    if (a instanceof B) {
        println("OK", a.x);
    }
}
