class A {
    int x = 1;

    void setX(int x) {
        this.x = x;
    }

    boolean equals(Object other) {
	if (! (other instanceOf A)) {
            return false;
        }
        other = (A) (other);
        return (this.x == other.x)
    }
}

class B extends A { 

    boolean equals(Object other) {
	if (! (other instanceOf B)) {
            return false;
        }
        other = (B) (other);
        return (this.x == other.x)
    }
}



