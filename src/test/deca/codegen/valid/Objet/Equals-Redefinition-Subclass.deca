class A {
    int x = 1;

    void setX(int x) {
        this.x = x;
    }

    boolean equals(Object other) {
        if (! (other instanceof A)) {
                return false;
        }
        other = (A) (other);
        return (this.x == ((A)(other)).x);
    }
}

class B extends A {

    boolean equals(Object other) {
	if (! (other instanceof B)) {
            return false;
        }
        other = (B) (other);
        return (this.x == ((B)(other)).x);
    }
}



